---
import GolangIcon from "@components/icons/GolangIcon.astro";
import PHPIcon from "@components/icons/PHPIcon.astro";
import TSIcon from "@components/icons/TSIcon.astro";
import JavaIcon from "@components/icons/JavaIcon.astro";
import PythonIcon from "@components/icons/PythonIcon.astro";
import TechStackSubSection from "./TechStackSubSection.astro";
import TechStackCard from "@components/TechStackCard.astro";
---

<TechStackSubSection title="Programming Language">
    <TechStackCard name="PHP" color="catpuccin-sky">
        <PHPIcon class="fill-white" width="24px" slot="icon" />
    </TechStackCard>

    <TechStackCard name="Go" color="catpuccin-sky">
        <GolangIcon class="fill-white" width="24px" slot="icon" />
    </TechStackCard>

    <TechStackCard name="Typescript" color="catpuccin-sky">
        <TSIcon class="fill-white" width="24px" slot="icon" />
    </TechStackCard>

    <TechStackCard name="Java" color="catpuccin-sky">
        <JavaIcon class="fill-white" width="24px" slot="icon" />
    </TechStackCard>

    <TechStackCard name="Python" color="catpuccin-sky">
        <PythonIcon class="fill-white" width="24px" slot="icon" />
    </TechStackCard>
</TechStackSubSection>
