---
import TechStackSubSection from "./TechStackSubSection.astro";
import BunJSIcon from "@components/icons/BunJSIcon.astro";
import ElysiaJsIcon from "@components/icons/ElysiaJsIcon.astro";
import ExpressIcon from "@components/icons/ExpressIcon.astro";
import ReactIcon from "@components/icons/ReactIcon.astro";
import GolangIcon from "@components/icons/GolangIcon.astro";
import LaravelIcon from "@components/icons/LaravelIcon.astro";
import CodeIgniterIcon from "@components/icons/CodeIgniterIcon.astro";
import SymfonyIcon from "@components/icons/SymfonyIcon.astro";
import NodeJSIcon from "@components/icons/NodeJSIcon.astro";
import TechStackCard from "@components/TechStackCard.astro";
---

<TechStackSubSection title="Library, Framework, and Runtime ">
    <TechStackCard name="Laravel" color="catpuccin-sky">
        <LaravelIcon width="24px" slot="icon" />
    </TechStackCard>

    <TechStackCard name="CodeIgniter" color="catpuccin-sky">
        <CodeIgniterIcon width="24px" slot="icon" />
    </TechStackCard>

    <TechStackCard name="Symfony" color="catpuccin-sky">
        <SymfonyIcon width="24px" slot="icon" />
    </TechStackCard>

    <TechStackCard name="Elysia" color="catpuccin-sky">
        <ElysiaJsIcon width="24px" slot="icon" />
    </TechStackCard>

    <TechStackCard name="Express" color="catpuccin-sky">
        <ExpressIcon width="24px" slot="icon" />
    </TechStackCard>

    <TechStackCard name="Gin-Gonic" color="catpuccin-sky">
        <GolangIcon width="24px" slot="icon" />
    </TechStackCard>

    <TechStackCard name="Echo Go" color="catpuccin-sky">
        <GolangIcon width="24px" slot="icon" />
    </TechStackCard>

    <TechStackCard name="React" color="catpuccin-sky">
        <ReactIcon width="24px" slot="icon" />
    </TechStackCard>

    <TechStackCard name="NodeJS" color="catpuccin-sky">
        <NodeJSIcon width="24px" slot="icon" />
    </TechStackCard>

    <TechStackCard name="Bun JS" color="catpuccin-sky">
        <BunJSIcon width="24px" slot="icon" />
    </TechStackCard>
</TechStackSubSection>
